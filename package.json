    "test:auth": "node --experimental-vm-modules node_modules/jest/bin/jest.js ./tests/auth",
    "test:practo": "node --experimental-vm-modules node_modules/jest/bin/jest.js ./tests/practice",
    "test:reqs": "node --experimental-vm-modules node_modules/jest/bin/jest.js ./tests/requests/questionsByCategory",
    "test:displays": "npx playwright test ./tests/displays",
    "test:errors": "npx playwright test ./tests/errors",
    "test:filters": "npx playwright test ./tests/filters",
    "test:models": "npx playwright test ./tests/models",
    "test:nav": "npx playwright test ./tests/navigation",
    "test:setup": "npx playwright test ./tests/setup",
    "test:routes": "npx playwright test ./tests/routes",
    "test:practice": "npx playwright test ./tests/pw-practice",
    "test:pwauth": "npx playwright test ./tests/pw-auth",
    "test:pwreqs": "npx playwright test ./tests/requests/pw-searchByCat",
    "fire": "flame run server.js",
    "fire:convert": "node ./flamegraph/platflame.js",
    "seed": "node seeds/index.js"
  },
  "dependencies": {
    "@datadog/pprof": "^5.10.0",
    "@platformatic/flame": "^1.0.0",
