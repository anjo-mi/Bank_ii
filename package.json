  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.1",
    "cors": "^2.8.5",
    "jest": "^30.1.3",
    "morgan": "^1.10.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "react-pprof": "^1.0.0",
    "supertest": "^7.1.4"
  }
